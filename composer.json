{
	"name": "silverstripe/forum",
	"description": "Forum module for SilverStripe.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "forum"],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Sean Harvey",
		"email": "sean@silverstripe.com"
	}, {
		"name": "Will Rossiter",
		"email": "will@fullscreen.io"
	}],
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"require": {
		"silverstripe/framework": "^4.0@dev",
		"silverstripe/cms": "^4.0@dev",
        "silverstripe-archive/silverstripe-bbcodeparser": "1.0.x-dev"
	},
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/zanderwar/silverstripe-bbcodeparser"
      }
    ],
	"autoload": {
		"psr-4": {
			"SilverStripe\\Forum\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SilverStripe\\Forum\\Tests\\": "tests/"
		}
	}
}
